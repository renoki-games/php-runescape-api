{
    "name": "renoki-games/php-runescape-api",
    "description": "Runescape API is a simple wrapper for Old School Runescape, written in PHP",
    "keywords": ["laravel", "php", "api", "wrapper", "old", "school", "runescape"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/renoki-games/php-runescape-api",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "nesbot/carbon": "^2.31",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "RenokiGames\\Runescape\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiGames\\Runescape\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "RenokiGames\\Runescape\\RunescapeServiceProvider"
            ]
        }
    }
}
